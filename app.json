{
  "expo": {
    "name": "Pomodoro Timer",
    "slug": "pomodoro-timer",
    "description": "Utilizado para ajudar no gerenciamento do tempo atrav√©s de ciclos pomodoros",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#005028"
    },
    "ios": {
      "supportsTablet": true,
      "icon": {
        "light": "./assets/ios-light.png",
        "dark": "./assets/ios-dark.png",
        "tinted": "./assets/ios-tinted.png"
      },
      "bundleIdentifier": "com.edubamoura.pomodorotimer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#062013"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.edubamoura.pomodorotimer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#005028",
          "image": "./assets/splash-icon-light.png",
          "dark": {
            "backgroundColor": "#062013",
            "image": "./assets/splash-icon-dark.png"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d9e5091a-e218-4979-ae01-617e324e058f"
      }
    }
  }
}
